//Read a string and print all character "a" to uppercase and print string. 
//If character "a" is not there in string.
#include<stdio.h>
void main()
{
	char str[20];
	int i;
	printf("Enter string ");
	gets(str);
	for(i=0;str[i]!='\0';i++)
	{
		if(str[i]=='a')
		{
	     	str[i]=str[i]-32;
	    }
	}
	puts(str);
}
INPUT:     aditya
OUTPUT:    AdityA
