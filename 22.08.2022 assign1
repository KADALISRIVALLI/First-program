#include<stdio.h>
main()
{
	int a[100][100];
	int i,j,r,c,sum=0;
	printf("Enter how many rows\n");
	scanf("%d",&r);
	printf("Enter how many coloumns\n");
	scanf("%d",&c);
	printf("Enter array elements\n");
	for(i=0;i<r;i++)
	{
		for(j=0;j<c;j++)
		{
			scanf("%d",&a[i][j]);
		}
	}
	for(i=0;i<r;i++)
	{
		sum=0;
		for(j=0;j<c;j++)
		{
			sum=sum+a[j][i];
		}
		printf("%d row sum=%d\n",i+1,sum);
	}
}
